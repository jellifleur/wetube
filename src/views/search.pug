extends layouts/main.pug
include mixins/videoBlock

block content
  .search__header
    h3.search__title Searching for:
      strong=searchingBy
  if videos.length === 0
    h5.search__nothing No Videos Found.
  else
    .search__videos.home-videos
      each item in videos
        +videoBlock({
          id:item.id,
          title:item.title,
          views:item.views,
          videoFile:item.fileUrl,
          creator:item.creator.name
        })